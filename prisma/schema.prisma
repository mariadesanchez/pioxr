generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Pio {
  id       Int      @id @default(autoincrement())
  nombre   String?  
  tipo     String
  imageUrl String?
  creadoEn DateTime @default(now())
}
